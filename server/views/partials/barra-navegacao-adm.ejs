<nav>
    <p class="rotulo-email-logado"><%=emailLogado%></p>
    <button style="float: right;" onclick="Sair()">Sair</button>
    <script>
        function Sair() {
            fetch("/logout", {
                method: "POST",
            });
            window.location.pathname = ''
        }
    </script>
    <div class="caixa-de-botoes">
        <a id="lk_relatorio" href="/relatorios">Relat√≥rios</a>
        <a id="lk_membros" href="/membros">Membros</a>
    </div>
    <script>
        let pathAutal = window.location.pathname

        switch (pathAutal) {
            case '/gerar-relatorios':
                document.getElementById("lk_gerar_rela").remove()
                break;
            case '/membros':
                document.getElementById("lk_membros").remove()
                break;
            case '/relatorios':
                document.getElementById("lk_relatorio").remove()
                break;
            case '/login':
                document.getElementById("lk_login").remove()
                break;
        }
    </script>
</nav>